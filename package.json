{
  "name": "chan-dongle-extended-client",
  "version": "0.4.2",
  "description": "client for chan-dongle-extended",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "postinstall": "npm run fetch-ami-definitions",
    "fetch-ami-definitions": "cd src/lib && mkdir -p fetched && cd fetched && wget -N https://raw.githubusercontent.com/garronej/chan-dongle-extended/master/src/lib/AmiUserEvents.ts",
    "tsc": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "test": "node ./dist/test/main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/chan-dongle-extended-client.git"
  },
  "keywords": [
    "chan_dongle",
    "asterisk",
    "multipart SMS",
    "phonebook",
    "contact",
    "PIN"
  ],
  "author": "Joseph Garrone",
  "license": "MIT",
  "dependencies": {
    "js-base64": "^2.1.9",
    "rejection-tracker": "garronej/rejection-tracker",
    "ts-ami": "github:garronej/ts-ami",
    "ts-events-extended": "garronej/ts-events-extended"
  },
  "devDependencies": {
    "@types/js-base64": "^2.1.5",
    "@types/node": "^4.2.2",
    "typescript": "^2.3.2"
  }
}
